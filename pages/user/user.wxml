  <text class="nickname">{{nickname}}</text>
</view>
<navigator url="/pages/loginpage/loginpage" wx:else>
  <view class="header">
    <image src="/image/ano_user.jpg" class="thumb"></image>
    <text class="nickname">未登录</text>
  </view>
</navigator>